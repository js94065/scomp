// booleanequal.decaf

class Program {

	void main() {
		boolean x, y;

		x = true;
		y = false;

		callout("printf", "%d == %d = %d\n", x, x, x == x);
		callout("printf", "%d == %d = %d\n", x, y, x == y);
		callout("printf", "%d == %d = %d\n", y, x, y == x);
		callout("printf", "%d == %d = %d\n", y, y, y == y);
	}

}
